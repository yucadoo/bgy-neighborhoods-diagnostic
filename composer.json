{
    "name": "yucadoo/bgy-neighborhoods-diagnostic",
    "type": "library",
    "description": "Error detection strategy for Boris GuÃ©ry's transient error handling using Neighborhoods' throwable diagnostic component.",
    "keywords": [
        "yucadoo",
        "transient fault",
        "retry"
    ],
    "homepage": "https://github.com/yucadoo/bgy-neighborhoods-diagnostic",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrvoje Jukic",
            "email": "hrcajuka@gmail.com",
            "homepage": "https://yuca.ba",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "borisguery/transient-error-handling": "^0.4.0",
        "neighborhoods/throwable-diagnostic-component": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "YucaDoo\\BgyNeighborhoodsDiagnostic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YucaDoo\\BgyNeighborhoodsDiagnostic\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
